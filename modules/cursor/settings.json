{
    "editor.fontFamily": "Operator Mono, 'Fira Code'",
    "editor.fontLigatures": true,
    "editor.fontSize": 17,
    "editor.lineHeight": 24,
    "editor.fontWeight": "400",
    "editor.letterSpacing": 0.5,
    "workbench.fontAliasing": "auto",
    "editor.renderWhitespace": "all",
    "editor.tabCompletion": "on",
    "editor.snippetSuggestions": "top",
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.formatOnPaste": false,
    "editor.autoIndent": "full",
    "editor.detectIndentation": true,
    "editor.wrappingIndent": "same",
    "editor.lineNumbers": "on",
    "editor.tabSize": 4,
    "editor.wordWrap": "on",
    "editor.formatOnType": false,
    "editor.suggestSelection": "first",
    "editor.quickSuggestionsDelay": 0,
    "editor.formatOnSave": false,
    "editor.copyWithSyntaxHighlighting": false,
    "[javascript]": {
        "editor.formatOnSave": true,
        "editor.formatOnPaste": false,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascriptreact]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[vue]": {
        "editor.formatOnSave": false,
        "editor.defaultFormatter": "Vue.volar"
    },
    "[ts]": {
        "editor.formatOnSave": false,
        "editor.formatOnPaste": false
    },
    "[ejs]": {
        "editor.formatOnSave": false
    },
    "[jsp]": {
        "editor.formatOnSave": false
    },
    "editor.codeActionsOnSave": {
        "source.fixAll": "explicit"
    },
    "window.title": "${dirty} ${activeEditorMedium}${separator}${rootName}",
    "window.titleBarStyle": "native",
    "window.restoreFullscreen": true,
    "window.newWindowDimensions": "inherit",
    "workbench.settings.editor": "json",
    "window.nativeFullScreen": false,
    "files.autoSave": "off",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/node_modules": true,
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true,
        "**/qa": true,
        "**/qa_data": true,
        "**/.next": true,
        ".yarn": true
    },
    "files.encoding": "utf8",
    "files.trimTrailingWhitespace": false,
    "files.associations": {
        ".env": "shell",
        ".jspt": "jsp",
        ".env*": "dotenv"
    },
    "terminal.explorerKind": "integrated",
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.shell.osx": "/bin/zsh",
    "terminal.integrated.fontFamily": "Operator Mono",
    "git.autofetch": true,
    "git.enableSmartCommit": true,
    "sync.gist": "c1b97423ef7ec45a9e9b5e8bfc5839b8",
    "sync.autoUpload": true,
    "sync.syncExtensions": true,
    "sync.autoDownload": true,
    "sync.quietSync": true,
    "breadcrumbs.enabled": true,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "search.useIgnoreFiles": true,
    "search.exclude": {
        ".next": true,
        ".cache": true,
        "public/build": true,
        "**/node_modules": true,
        "**/dist": true,
        "**/.jest-cache": true,
        ".jest-cache": true,
        "**/.venv": true,
        "pnpm-lock.yaml": true
    },
    "zenMode.fullScreen": false,
    "editor.suggestFontSize": 16,
    "editor.suggestLineHeight": 28,
    "git.autorefresh": true,
    "[markdown]": {
        "editor.wordWrap": "on",
        "editor.quickSuggestions": {
            "comments": "on",
            "strings": "on",
            "other": "on"
        }
    },
    "files.defaultLanguage": "markdown",
    "multiCommand.commands": [
        {
            "command": "multiCommand.focusMode",
            "sequence": [
                "workbench.action.toggleZenMode"
            ]
        }
    ],
    "extensions.ignoreRecommendations": true,
    "editor.renderControlCharacters": false,
    "scm.diffDecorations": "none",
    "editor.matchBrackets": "never",
    "workbench.tips.enabled": false,
    "editor.lightbulb.enabled": "off",
    "editor.selectionHighlight": false,
    "editor.overviewRulerBorder": false,
    "editor.renderLineHighlight": "none",
    "editor.occurrencesHighlight": "off",
    "editor.hideCursorInOverviewRuler": true,
    "editor.gotoLocation.multipleReferences": "goto",
    "editor.gotoLocation.multipleDefinitions": "goto",
    "editor.gotoLocation.multipleDeclarations": "goto",
    "editor.gotoLocation.multipleImplementations": "goto",
    "editor.gotoLocation.multipleTypeDefinitions": "goto",
    "terminal.integrated.fontSize": 14,
    "diffEditor.ignoreTrimWhitespace": false,
    "diffEditor.renderSideBySide": false,
    "files.trimFinalNewlines": true,
    "files.insertFinalNewline": true,
    "window.nativeTabs": true,
    "customizeUI.titleBar": "inline",
    "customizeUI.stylesheet": {
        ".editor .title": "background: transparent !important;",
        ".editor .title .label-container": "visibility: hidden;",
        ".editor .title .actions-container .action-item a": "visibility:hidden;",
        ".editor .title .dirty .actions-container .action-item a": "visibility:initial;",
        ".editor .title .actions-container .action-item a[title=\"Open Settings (UI)\"]": "visibility: initial;",
        ".editor .title .actions-container .action-item a[title=\"Open Settings (JSON)\"]": "visibility: initial;",
        ".editor .title .actions-container": "flex-direction: row-reverse;",
        ".editor .scrollbar .slider": "visibility: hidden",
        ".editor .scrollbar:hover .slider": "visibility: initial",
        ".monaco-editor .cursor": "background: linear-gradient(to bottom, #7f00ff, #e100ff) !important; color: #292D3E !important"
    },
    "editor.wordSeparators": "`~!@#%^&*()=+[{]}\\|;:'\",.<>/?",
    "emmet.includeLanguages": {
        "vue": "html"
    },
    "prettier.useEditorConfig": false,
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "workbench.startupEditor": "newUntitledFile",
    "workbench.editor.tabCloseButton": "off",
    "[typescript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "editor.accessibilitySupport": "off",
    "editor.inlineSuggest.enabled": true,
    "security.workspace.trust.untrustedFiles": "open",
    "editor.guides.indentation": false,
    "workbench.colorTheme": "Tomorrow Night Blue",
    "terminal.integrated.env.osx": {
        "FIG_NEW_SESSION": "1"
    },
    "workbench.iconTheme": "material-icon-theme",
    "editor.linkedEditing": true,
    "git.confirmSync": false,
    "vim.startInInsertMode": true,
    "vim.leader": "<space>",
    "github.copilot.enable": {
        "*": false,
        "plaintext": false,
        "markdown": false,
        "scminput": false,
        "mdx": false,
        "md": false
    },
    "vue.codeActions.enabled": false,
    "editor.minimap.enabled": false,
    "editor.wordWrapColumn": 80,
    "console-ninja.featureSet": "Community",
    "custom-language-properties": {
        "scss.comments.lineComment": ""
    },
    "Notes.notesLocation": "/Users/ashiknesin/Sync/Notes/vs-code",
    "workbench.editor.tabActionCloseVisibility": false,
    "codeium.enableCodeLens": false,
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[mdx]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "prisma.showPrismaDataPlatformNotification": false,
    "github.copilot.editor.enableAutoCompletions": true,
    "continue.enableTabAutocomplete": false,
    "git.ignoreRebaseWarning": true,
    "codeviz.analyticsEnabled": true,
    "git.openRepositoryInParentFolders": "always",
    "github.copilot-labs.advanced": {},
    "github.copilot.chat.commitMessageGeneration.instructions": [],
    "github.copilot.chat.search.semanticTextResults": true,
    "roo-cline.allowedCommands": [
        "npm test",
        "npm install",
        "tsc",
        "git log",
        "git diff",
        "git show"
    ],
    "redhat.telemetry.enabled": false,
    "cursor.cpp.disabledLanguages": [
        "plaintext",
        "markdown",
        "scminput",
        "mdx",
        "md"
    ],
    "editor.tokenColorCustomizations": {
        "[*Light*]": {
            "textMateRules": [
                {
                    "scope": "ref.matchtext",
                    "settings": {
                        "foreground": "#000"
                    }
                }
            ]
        },
        "[*Dark*]": {
            "textMateRules": [
                {
                    "scope": "ref.matchtext",
                    "settings": {
                        "foreground": "#fff"
                    }
                }
            ]
        },
        "textMateRules": []
    },
    "dotenv.enableAutocloaking": false,
    "cursor.composer.shouldAllowCustomModes": true,
    "github.copilot.selectedCompletionModel": "gpt-4o-copilot",
    "superdesign.aiModelProvider": "openrouter",
    "kilo-code.allowedCommands": [
        "git log",
        "git diff",
        "git show",
        "npm test",
        "npm install",
        "tsc"
    ],
    "kilo-code.deniedCommands": [],
    "cursor.enable_agent_window_setting": true,
    "[dockercompose]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 2,
        "editor.autoIndent": "advanced",
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "[github-actions-workflow]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "amp.url": "https://ampcode.com/",
    "amp.tab.enabled": true,
    "editor.wordBasedSuggestions": "off",
    "zenMode.fullScreen": true,
    "zenMode.centerLayout": false,
    "git.replaceTagsWhenPull": true
}
